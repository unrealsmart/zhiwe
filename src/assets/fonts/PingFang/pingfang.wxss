@font-face {
    font-family: "PingFangMedium";
    src: url('PingFang Medium.ttf'); /* IE9*/
    src: url('PingFang Medium.ttf?t=1484311526877#iefix') format('embedded-opentype'), /* IE6-IE8 */
    url('PingFang Medium.ttf?t=1484311526877') format('woff'), /* chrome, firefox */
    url('PingFang Medium.ttf?t=1484311526877') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
    url('PingFang Medium.ttf?t=1484311526877#iconfont') format('svg'); /* iOS 4.1- */
}

@font-face {
    font-family: "PingFangHeavy";
    src: url('PingFang Heavy.ttf'); /* IE9*/
    src: url('PingFang Heavy.ttf?t=1484311526877#iefix') format('embedded-opentype'), /* IE6-IE8 */
    url('PingFang Heavy.ttf?t=1484311526877') format('woff'), /* chrome, firefox */
    url('PingFang Heavy.ttf?t=1484311526877') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
    url('PingFang Heavy.ttf?t=1484311526877#iconfont') format('svg'); /* iOS 4.1- */
}

.pingfang-medium {
    font-family: "PingFangMedium";
    font-size: 12px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.pingfang-heavy {
    font-family: "PingFangHeavy";
    font-size: 12px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}